{% extends "base.html.twig" %}

{% block title %}
	{{ parent() }}
	Boutique
{% endblock %}

{% block nav_class %}
	transparent navbar-dark navbar-bg-dark
{% endblock %}

{% block body %}
	
	<section class="wrapper bg-light">
		{% for zasshi in zasshis %}
			<div class="container pb-14 pb-md-16 pt-12 text-center">
				<h2>{{ zasshi.name }}</h2>
				<embed src="{{ zasshi.pdf }}" width="800" height="500" type="application/pdf"/>
			</div>
		{% endfor %}
	</section>

{% endblock body %}
